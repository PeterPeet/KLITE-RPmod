{
  "name": "klite-rpmod",
  "version": "1.0.0",
  "description": "KLITE-Tools is a combination of \"KoboldAI Lite\" and \"Tools\", because this are user-mods for KoboldAI Lite. They started as a tool to create a black theme for Lite, but grew more and more to a toolset that can do different actions and minor changes to the Lite UI.",
  "main": "KLITE-RPmod_ALPHA.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "node tests/run_tests.js",
    "start": "node tests/run_tests.js",
    "test:functional": "node tests/run_tests.js functional",
    "test:integration": "node tests/run_tests.js integration",
    "test:performance": "node tests/run_tests.js performance",
    "test:data": "node tests/run_tests.js data",
    "lint": "eslint . --ext .js",
    "lint:report": "eslint . --ext .js -f json -o eslint-report.json",
    "lint:sarif": "eslint . --ext .js -f sarif -o eslint-report.sarif"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jsdom": "^24.1.3",
    "eslint": "^9.9.0",
    "eslint-formatter-sarif": "^2.1.7"
  }
}